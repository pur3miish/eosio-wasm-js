  "files": [
    "serialize.mjs",
    "utils/*.mjs",
    "actions.mjs",
    "asset.mjs",
    "block_time_stamp.mjs",
    "bool.mjs",
    "bytes.mjs",
    "checksum.mjs",
    "extended_asset.mjs",
    "float32.mjs",
    "float64.mjs",
    "float128.mjs",
