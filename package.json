{
  "name": "eosio-wasm-js",
  "version": "2.1.1",
  "description": "A utility tool that serialises EOSIO based blockchain types to WebAssembly hex string.",
  "main": "public/index.js",
  "keywords": [
    "EOSIO",
    "blockchain",
    "webassembly",
    "serialize",
    "types",
    "javascript"
  ],
  "author": "pur3miish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pur3miish/eosio-wasm-js/issues"
  },
  "homepage": "https://github.com/pur3miish/eosio-wasm-js#readme",
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier --write readme.md && prettier --write changelog.md",
    "test": "npm run test:api && npm run test:eslint && npm run test:prettier",
    "test:api": "coverage-node test/index.test.mjs",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "prepublishOnly": "npm test"
  },
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "> 0.5%, not OperaMini all, not IE > 0, not dead"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
