  "files": [
    "index.js",
    "utils",
    "actions.js",
    "asset.js",
    "block_time_stamp.js",
    "bool.js",
    "bytes.js",
    "checksum.js",
    "extended_asset.js",
    "float32.js",
    "float64.js",
    "float128.js",
    "int.js",
    "name.js",
    "public_key.js",
    "signature.js",
    "string.js",
    "symbol.js",
    "symbol_code.js",
    "time_point.js",
    "time_point_sec.js",
    "uint.js",
    "varint32.js",
    "varuint32.js",
    "transaction_header.js",
    "permission.js"
