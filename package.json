{
  "name": "eosio-wasm-js",
  "version": "5.0.0",
  "description": "A utility tool that serialises EOSIO and Antelope based blockchain action data to WebAssembly hex string.",
  "keywords": [
    "EOSIO",
    "Antelope",
    "blockchain",
    "webassembly",
    "serialize",
    "types",
    "javascript"
  ],
  "type": "module",
  "author": "pur3miish",
  "license": "MIT",
  "bugs": "https://github.com/pur3miish/eosio-wasm-js/issues",
  "homepage": "https://github.com/pur3miish/eosio-wasm-js#readme",
  "scripts": {
    "eslint": "eslint .",
    "prettier": "prettier -c .",
    "prettier:readme": "prettier --write readme.md",
    "types": "tsc -p jsconfig.json",
    "tests": "mocha",
    "test": "npm run eslint && npm run prettier && npm run types && npm run tests",
    "prepublishOnly": "npm test"
  },
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "> 0.5%, not OperaMini all, not IE > 0, not dead"
  ],
  "sideEffects": false,
  "exports": {
    "./serialize": "./serialize.js",
    "./package.json": "./package.json",
    "./actions": "./actions.js",
    "./asset": "./asset.js",
    "./block_time_stamp": "./block_time_stamp.js",
    "./bool": "./bool.js",
    "./bytes": "./bytes.js",
    "./checksum": "./checksum.js",
    "./extended_asset": "./extended_asset.js",
    "./float32": "./float32.js",
    "./float64": "./float64.js",
    "./float128": "./float128.js",
    "./int": "./int.js",
    "./name": "./name.js",
    "./permission": "./permission.js",
    "./public_key": "./public_key.js",
    "./signature": "./signature.js",
    "./string": "./string.js",
    "./symbol_code": "./symbol_code.js",
    "./symbol": "./symbol.js",
    "./time_point_sec": "./time_point_sec.js",
    "./time_point": "./time_point.js",
    "./transaction_header": "./transaction_header.js",
    "./uint": "./uint.js",
    "./varint32": "./varint32.js",
    "./varuint32": "./varuint32.js"
  },
  "files": [
    "serialize.js",
    "utils/*.js",
    "actions.js",
    "asset.js",
    "block_time_stamp.js",
    "bool.js",
    "bytes.js",
    "checksum.js",
    "extended_asset.js",
    "float32.js",
    "float64.js",
    "float128.js",
    "int.js",
    "name.js",
    "public_key.js",
    "signature.js",
    "string.js",
    "symbol.js",
    "symbol_code.js",
    "time_point.js",
    "time_point_sec.js",
    "uint.js",
    "varint32.js",
    "varuint32.js",
    "transaction_header.js",
    "permission.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pur3miish/eosio-wasm-js.git"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "base58-js": "^3.0.0",
    "chai": "^5.1.2",
    "coverage-node": "^8.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mocha": "^11.0.1",
    "prettier": "^2.8.4",
    "ripemd160-js": "^3.0.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "base58-js": "^3.0.0",
    "ripemd160-js": "^3.0.1"
  }
}
